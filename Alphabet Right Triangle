import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        for(int i=0;i<a;i++){
            char b=(char)('A' +i);
            for(int j=0;j<=i;j++){
                System.out.print((char)(b +j)+" ");
            }
            System.out.println();
            
        }
    }
}
